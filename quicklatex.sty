% General Setting of listings
\lstset{
    basicstyle=\ttfamily\footnotesize,
    frame=single,                     % 边框设置
    rulecolor=\color{black},          % 边框颜色
    framesep=3pt,                     % 边框与代码的距离
    framerule=0.5pt,                  % 边框线的宽度
    showstringspaces=false,           % 不显示字符串中的空格符号
    tabsize=4,                        % Tab键的宽度
    captionpos=b,                     % 标题位置在底部(bottom)
    breaklines=true,                  % 自动换行
    breakatwhitespace=false,          % 换行只在空格处
    title=\lstname,                   % 显示代码文件名
    aboveskip=3mm,
    belowskip=3mm,
    columns=flexible,
    keepspaces=true                   % 保持空格不变，有助于保持原有的格式
    escapeinside={\%*}{*)},
    backgroundcolor=\color{white},  % 背景颜色
}

